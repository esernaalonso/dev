{
  "exclude": [
    "^(node_modules|bower_components|\\.imdone|target|build|dist|logs)[\\/\\\\]?|\\.(git|svn|hg|npmignore)|\\~$|\\.(jpg|png|gif|swp|ttf|otf)$"
  ],
  "watcher": true,
  "code": {
    "include_lists": [
      "TODO",
      "INPROGRESS",
      "BLOCKED",
      "TESTING",
      "DONE"
    ]
  },
  "lists": [
    {
      "name": "TODO",
      "hidden": false
    },
    {
      "name": "INPROGRESS",
      "hidden": false
    },
    {
      "name": "BLOCKED",
      "hidden": false
    },
    {
      "name": "TESTING",
      "hidden": false
    },
    {
      "name": "DONE",
      "hidden": false
    }
  ],
  "marked": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "smartLists": true,
    "langPrefix": "language-"
  }
}